{
  "name": "line-to-column",
  "displayName": "Line > Column",
  "description": "Split lines to column",
  "version": "0.0.1",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-line-to-column"
  },
  "homepage": "https://github.com/lamartire/vscode-line-to-column",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "line-to-column.splitLine",
      "title": "Split line to column"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
