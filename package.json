{
  "name": "line-to-column",
  "displayName": "Line > Column",
  "description": "Split lines to column",
  "version": "0.0.1",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-line-to-column"
  },
  "homepage": "https://github.com/lamartire/vscode-line-to-column",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "icon.png",
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "line-to-column.splitLine",
      "title": "Split line to column"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.4.5",
    "vscode": "^1.0.0",
    "mocha": "^6.1.4",
    "@types/node": "^12.0.0",
    "@types/mocha": "^5.2.6"
  }
}
